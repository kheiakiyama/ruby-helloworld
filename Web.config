<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Proxy" stopProcessing="true">
          <match url="^(.*)$" ignoreCase="false" />
          <action type="Rewrite" url="{URL}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
